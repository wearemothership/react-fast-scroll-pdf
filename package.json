{
  "name": "react-fast-scroll-pdf",
  "version": "0.1.3",
  "description": "Image-based PDF viewer using pdfjs",
  "author": "mothershipsoft",
  "license": "MIT",
  "repository": "mothershipsoft/react-fast-scroll-pdf",
  "main": "dist/main/",
  "module": "dist/esm/",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rm -r -f ./dist/* && rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "start": "rm -r -f ./dist/* && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist && rollup -c -w ",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "example": "npm run build && cd example && npm install && npm start"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "pdfjs-dist": "^2.6.347",
    "react-html-parser": "^2.0.2"
  },
  "peerDependencies": {
    "react": "~17.0.0"
  },
  "devDependencies": {
    "react": "^17.0.2",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.3.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.3",
    "@types/react-html-parser": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.0",
    "gh-pages": "^3.1.0",
    "postcss": "^8.2.8",
    "react-scripts": "^4.0.3",
    "rollup": "^2.42.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "tslib": "^2.0.1",
    "typescript": "^4.2.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-transform-runtime"
    ]
  }
}
