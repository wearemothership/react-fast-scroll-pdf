{
	"name": "react-fast-scroll-pdf",
	"version": "1.3.1",
	"description": "Image-based PDF viewer using pdfjs",
	"author": "wearemothership",
	"license": "MIT",
	"keywords": [
		"react",
		"pdf",
		"scroll",
		"ui",
		"react-component",
		"pdfjs",
		"pdf.js"
	],
	"repository": "wearemothership/react-fast-scroll-pdf",
	"main": "dist/index.js",
	"module": "dist/index.module.js",
	"types": "dist/index.d.ts",
	"engines": {
		"node": ">=18",
		"npm": ">=9"
	},
	"scripts": {
		"build": "rimraf ./dist && rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
		"prepare": "npm run build",
		"predeploy": "cd example && npm install && npm run build",
		"deploy": "gh-pages -d example/build",
		"example": "npm run build && cd example && npm install && npm start",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"dependencies": {
		"html-react-parser": "^5.1.10",
		"immer": "^10.1.1",
		"lodash": "^4.17.21",
		"pdfjs-dist": "^4.3.136"
	},
	"peerDependencies": {
		"react": "^18.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.24.7",
		"@babel/plugin-transform-react-jsx": "^7.24.7",
		"@babel/plugin-transform-runtime": "^7.24.7",
		"@babel/preset-env": "^7.24.7",
		"@babel/preset-react": "^7.24.7",
		"@babel/runtime": "^7.24.7",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-image": "^3.0.3",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"@stylistic/eslint-plugin": "^5.2.2",
		"@types/lodash": "^4.17.5",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/react-html-parser": "^2.0.6",
		"@vitejs/plugin-react": "^4.3.1",
		"@wearemothership/eslint-config": "github:wearemothership/eslint-config",
		"eslint": "^9.32.0",
		"eslint-plugin-import": "^2.32.0",
		"gh-pages": "^6.1.1",
		"nan": "^2.20.0",
		"postcss": "^8.4.38",
		"react": "^18.3.1",
		"rimraf": "^5.0.7",
		"rollup": "^4.18.0",
		"rollup-plugin-copy": "^3.5.0",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-postcss": "^4.0.2",
		"tslib": "^2.6.3",
		"typescript": "^5.5.2"
	},
	"babel": {
		"presets": [
			"@babel/preset-react",
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/plugin-transform-react-jsx",
			"@babel/plugin-transform-runtime"
		]
	},
	"exports": {
		".": {
			"require": "./dist/index.js",
			"default": "./dist/index.module.js",
			"types": "./dist/index.d.ts"
		}
	}
}
