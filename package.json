{
	"name": "react-fast-scroll-pdf",
	"version": "1.4.0",
	"type": "module",
	"description": "Image-based PDF viewer using pdfjs",
	"author": "wearemothership",
	"license": "MIT",
	"keywords": [
		"react",
		"pdf",
		"scroll",
		"ui",
		"react-component",
		"pdfjs",
		"pdf.js"
	],
	"repository": "wearemothership/react-fast-scroll-pdf",
	"main": "dist/index.js",
	"module": "dist/index.module.js",
	"types": "dist/index.d.ts",
	"engines": {
		"node": ">=18",
		"npm": ">=9"
	},
	"scripts": {
		"build": "rimraf ./dist && rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
		"prepare": "npm run build",
		"predeploy": "cd example && npm install && npm run build",
		"deploy": "gh-pages -d example/build",
		"example": "npm run build && cd example && npm install && npm start",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"dependencies": {
		"html-react-parser": "^5.2.6",
		"immer": "^10.1.1",
		"lodash": "^4.17.21",
		"pdfjs-dist": "^5.4.54"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.28.0",
		"@babel/plugin-transform-react-jsx": "^7.27.1",
		"@babel/plugin-transform-runtime": "^7.28.0",
		"@babel/preset-env": "^7.28.0",
		"@babel/preset-react": "^7.27.1",
		"@babel/runtime": "^7.28.2",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^28.0.6",
		"@rollup/plugin-image": "^3.0.3",
		"@rollup/plugin-inject": "^5.0.5",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@rollup/plugin-typescript": "^12.1.4",
		"@types/lodash": "^4.17.20",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"@types/react-html-parser": "^2.0.7",
		"@vitejs/plugin-react": "^4.7.0",
		"@wearemothership/eslint-config": "github:wearemothership/eslint-config",
		"gh-pages": "^6.3.0",
		"postcss": "^8.5.6",
		"react": "^19.1.1",
		"rimraf": "^6.0.1",
		"rollup": "^4.46.2",
		"rollup-plugin-copy": "^3.5.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-postcss": "^4.0.2",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3"
	},
	"babel": {
		"presets": [
			"@babel/preset-react",
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/plugin-transform-react-jsx",
			"@babel/plugin-transform-runtime"
		]
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"default": "./dist/index.module.js"
		}
	}
}
